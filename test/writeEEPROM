//Data pin mapping
//					D0, D1, D2...            ...D7
static int DQ[8] = {47, 48, 49, 35, 34, 33, 32, 31};
//Address pin mapping
//					A0, A1, A2...                                       ...A14, A15, A16
static int A[17] = {46, 45, 44, 43, 42, 41, 40, 39, 25, 26, 29, 27, 38, 24, 23, 37, 36};
static int WE = 22;
static int OE = 28;
static int CE = 30;

void setup() {
	Serial.begin(115200);
	for (int i = 0; i < 8; i++)
		pinMode(DQ[i], OUTPUT);
	for (int i = 0; i < 17; i++)
		pinMode(A[i], OUTPUT);
}

void loop() {
	if (Serial.available() > 0) {
		
	}
}